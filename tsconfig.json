{
  "compilerOptions": {
    "module": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true,
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "isolatedModules": true,
    "allowJs": true,
    "baseUrl": "./",
    "noEmit": true,
    "lib": [
      "DOM",
      "ESNext"
    ],
    "target": "ESNext",
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx",
    "babel.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
